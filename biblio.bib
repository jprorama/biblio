@Article{Bedi2012,
  Title                    = {Trust based recommender system using ant colony for trust computation},
  Author                   = {Punam Bedi and Ravish Sharma},
  Journal                  = {Expert Systems with Applications},
  Year                     = {2012},
  Number                   = {1},
  Pages                    = {1183 - 1190},
  Volume                   = {39},

  Abstract                 = {Collaborative Filtering (CF) technique has proven to be promising for implementing large scale recommender systems but its success depends mainly on locating similar neighbors. Due to data sparsity of the user‚Äìitem rating matrix, the process of finding similar neighbors does not often succeed. In addition to this, it also suffers from the new user (cold start) problem as finding possible neighborhood and giving recommendations to user who has not rated any item or rated very few items is difficult. In this paper, our proposed Trust based Ant Recommender System (TARS) produces valuable recommendations by incorporating a notion of dynamic trust between users and selecting a small and best neighborhood based on biological metaphor of ant colonies. Along with the predicted ratings, displaying additional information for explanation of recommendations regarding the strength and level of connectedness in trust graph from where recommendations are generated, items and number of neighbors involved in predicting ratings can help active user make better decisions. Also, new users can highly benefit from pheromone updating strategy known from ant algorithms as positive feedback in the form of aggregated dynamic trust pheromone defines ‚Äúpopularity‚Äù of a user as recommender over a period of time. The performance of TARS is evaluated using two datasets of different sparsity levels viz. Jester dataset and MovieLens dataset (available online) and compared with traditional Collaborative Filtering based approach for generating recommendations.},
  Doi                      = {https://doi.org/10.1016/j.eswa.2011.07.124},
  File                     = {Bedi2012.pdf:./pdf/Bedi2012.pdf:PDF},
  ISSN                     = {0957-4174},
  Keywords                 = {Collaborative Filtering, Trust, Recommender system, Ant colony, Pheromone updating},
  Owner                    = {jpr},
  Timestamp                = {2019.02.23},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0957417411010864}
}

