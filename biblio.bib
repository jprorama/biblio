@Article{Figueroa2015,
  Title                    = {A systematic literature review of Linked Data-based recommender systems},
  Author                   = {Figueroa, Cristhian and Vagliano, Iacopo and Rocha, Oscar Rodríguez and Morisio, Maurizio},
