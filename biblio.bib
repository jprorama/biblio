@InBook{Beel2014,
  Title                    = {Utilizing Mind-Maps for Information Retrieval and User Modelling},
  Author                   = {Beel, Joeran
and Langer, Stefan
and Genzmehr, Marcel
and Gipp, Bela},
  Editor                   = {Dimitrova, Vania
and Kuflik, Tsvi
